{
  "manifest_version": 2,

  "name": "SYP",
  "description": "This extension grabs songs from a spotify playlist and provides a youtube link containing music videos for each song.",
  "version": "0.2.0",

  "browser_action": {
    "default_icon": "s_full.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "background",
    "https://www.youtube.com/"
  ],
  "icons": {
    "16": "s_full.png",
    "48": "s_full.png",
    "128": "s_full.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
